{
    "colors": {
      "primary": "#1E1E1E",
      "secondary": "#5A5A5A",
      "background": "#FDFDFD",
      "surface": "#F8F9FA",
      "border": "#EAEAEA",
      "text": {
        "primary": "#1E1E1E",
        "secondary": "#5A5A5A",
        "muted": "#8E8E8E",
        "onPrimary": "#FFFFFF"
      },
      "accent": {
        "lightBlueGray": "#DDE2E6",
        "teal": "#89C6BD",
        "limeGreen": "#D4E5C4",
        "skyBlue": "#8AB8D7"
      },
      "rating": {
        "star": "#F2C94C",
        "starEmpty": "#E0E0E0"
      }
    },
    "typography": {
      "fontFamily": {
        "sans": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
        "logo": "'Manrope', sans-serif"
      },
      "styles": {
        "logo": {
          "fontFamily": "var(--font-logo)",
          "fontSize": "22px",
          "fontWeight": 700,
          "letterSpacing": "-0.02em"
        },
        "display": {
          "fontFamily": "var(--font-sans)",
          "fontSize": "48px",
          "fontWeight": 700,
          "lineHeight": "1.2",
          "letterSpacing": "-0.02em"
        },
        "pageTitle": {
          "fontFamily": "var(--font-sans)",
          "fontSize": "36px",
          "fontWeight": 700,
          "lineHeight": "1.25"
        },
        "sectionTitle": {
          "fontFamily": "var(--font-sans)",
          "fontSize": "28px",
          "fontWeight": 600,
          "lineHeight": "1.3"
        },
        "cardTitle": {
            "fontFamily": "var(--font-sans)",
            "fontSize": "16px",
            "fontWeight": 500,
            "lineHeight": "1.4"
        },
        "priceLarge": {
            "fontFamily": "var(--font-sans)",
            "fontSize": "24px",
            "fontWeight": 500
        },
        "priceSmall": {
            "fontFamily": "var(--font-sans)",
            "fontSize": "14px",
            "fontWeight": 500,
            "color": "var(--color-text-secondary)"
        },
        "body": {
          "fontFamily": "var(--font-sans)",
          "fontSize": "14px",
          "fontWeight": 400,
          "lineHeight": "1.6"
        },
        "label": {
            "fontFamily": "var(--font-sans)",
            "fontSize": "12px",
            "fontWeight": 500,
            "color": "var(--color-text-secondary)",
            "textTransform": "uppercase",
            "letterSpacing": "0.05em"
        },
        "caption": {
            "fontFamily": "var(--font-sans)",
            "fontSize": "12px",
            "fontWeight": 400,
            "color": "var(--color-text-muted)"
        }
      }
    },
    "spacing": {
      "xs": "4px",
      "sm": "8px",
      "md": "16px",
      "lg": "24px",
      "xl": "32px",
      "xxl": "48px",
      "xxxl": "64px"
    },
    "radii": {
      "sm": "4px",
      "md": "8px",
      "lg": "12px",
      "full": "9999px"
    },
    "shadows": {
      "sm": "0px 2px 4px rgba(0, 0, 0, 0.04)",
      "md": "0px 5px 15px rgba(0, 0, 0, 0.06)",
      "lg": "0px 10px 30px rgba(0, 0, 0, 0.08)",
      "inner": "inset 0px 2px 4px rgba(0, 0, 0, 0.05)"
    },
    "components": {
      "button": {
        "primary": {
          "backgroundColor": "var(--color-primary)",
          "color": "var(--color-text-on-primary)",
          "borderRadius": "var(--radius-full)",
          "padding": "14px 40px",
          "fontSize": "14px",
          "fontWeight": 600
        },
        "secondary": {
            "backgroundColor": "transparent",
            "color": "var(--color-text-secondary)",
            "border": "1px solid var(--color-border)",
            "borderRadius": "var(--radius-full)",
            "padding": "10px 20px",
            "fontSize": "12px",
            "fontWeight": 500
        },
        "icon": {
          "backgroundColor": "var(--color-primary)",
          "color": "var(--color-text-on-primary)",
          "borderRadius": "var(--radius-full)",
          "width": "40px",
          "height": "40px"
        }
      },
      "productCard": {
        "backgroundColor": "var(--color-background)",
        "borderRadius": "var(--radius-lg)",
        "boxShadow": "var(--shadow-md)"
      },
      "colorSwatch": {
        "width": "28px",
        "height": "28px",
        "borderRadius": "var(--radius-full)",
        "border": "3px solid var(--color-background)",
        "boxShadow": "0px 2px 4px rgba(0,0,0,0.1), 0 0 0 1px var(--color-border)"
      },
      "rating": {
        "starSize": "16px",
        "gap": "4px",
        "fontSize": "14px",
        "color": "var(--color-text-secondary)"
      },
      "accordion": {
        "border": "1px solid var(--color-border)",
        "borderRadius": "var(--radius-md)"
      },
      "tag": {
        "backgroundColor": "var(--color-surface)",
        "color": "var(--color-text-secondary)",
        "borderRadius": "var(--radius-full)",
        "padding": "6px 12px",
        "fontSize": "12px",
        "fontWeight": 500
      }
    },
    "layout": {
      "containerWidth": "1240px",
      "gridGap": "32px",
      "breakpoints": {
        "sm": "640px",
        "md": "768px",
        "lg": "1024px",
        "xl": "1280px"
      }
    }
  } 