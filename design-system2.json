{
    "colors": {
      "primary": "#1E1E1E",
      "secondary": "#5A5A5A",
      "background": "#FFFFFF",
      "surface": "#F8F9FA",
      "border": "#E6E6E6",
      "lightBackground": "#F5F5F5",
      "text": {
        "primary": "#1E1E1E",
        "secondary": "#5A5A5A",
        "muted": "#8E8E8E"
      },
      "accent": {
        "lightBlue": "#B8D4E3",
        "teal": "#7EC8BD",
        "lightGreen": "#C8E6C9",
        "blue": "#6BA6CD",
        "paleBlue": "#E3F2FD",
        "warmWhite": "#FEFEFE"
      },
      "rating": {
        "star": "#FFD700",
        "starEmpty": "#E6E6E6"
      }
    },
    "typography": {
      "fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
      "logo": {
        "fontFamily": "'Inter', sans-serif",
        "fontSize": "20px",
        "fontWeight": 600,
        "letterSpacing": "-0.02em"
      },
      "headings": {
        "h1": {
          "fontSize": "32px",
          "fontWeight": 700,
          "lineHeight": "1.25",
          "letterSpacing": "-0.02em"
        },
        "h2": {
          "fontSize": "24px",
          "fontWeight": 600,
          "lineHeight": "1.3",
          "letterSpacing": "-0.01em"
        },
        "h3": {
          "fontSize": "18px",
          "fontWeight": 600,
          "lineHeight": "1.4"
        },
        "h4": {
          "fontSize": "16px",
          "fontWeight": 600,
          "lineHeight": "1.4"
        }
      },
      "body": {
        "large": {
          "fontSize": "16px",
          "fontWeight": 400,
          "lineHeight": "1.6"
        },
        "regular": {
          "fontSize": "14px",
          "fontWeight": 400,
          "lineHeight": "1.5"
        },
        "small": {
          "fontSize": "12px",
          "fontWeight": 400,
          "lineHeight": "1.4"
        }
      },
      "product": {
        "title": {
          "fontSize": "20px",
          "fontWeight": 600,
          "lineHeight": "1.3",
          "letterSpacing": "-0.01em"
        },
        "price": {
          "fontSize": "18px",
          "fontWeight": 700,
          "color": "#1E1E1E"
        },
        "name": {
          "fontSize": "14px",
          "fontWeight": 500,
          "lineHeight": "1.4"
        },
        "category": {
          "fontSize": "12px",
          "fontWeight": 500,
          "color": "#5A5A5A",
          "textTransform": "uppercase",
          "letterSpacing": "0.05em"
        }
      }
    },
    "spacing": {
      "xs": "4px",
      "sm": "8px",
      "md": "12px",
      "lg": "16px",
      "xl": "24px",
      "xxl": "32px",
      "xxxl": "48px",
      "xxxxl": "64px"
    },
    "radii": {
      "xs": "2px",
      "sm": "4px",
      "md": "8px",
      "lg": "12px",
      "xl": "16px",
      "xxl": "24px",
      "full": "9999px"
    },
    "shadows": {
      "sm": "0px 2px 4px rgba(0, 0, 0, 0.04)",
      "md": "0px 4px 12px rgba(0, 0, 0, 0.06)",
      "lg": "0px 8px 24px rgba(0, 0, 0, 0.08)",
      "card": "0px 2px 8px rgba(0, 0, 0, 0.04)"
    },
    "components": {
      "button": {
        "primary": {
          "backgroundColor": "#1E1E1E",
          "color": "#FFFFFF",
          "borderRadius": "24px",
          "padding": "12px 32px",
          "fontSize": "14px",
          "fontWeight": 600,
          "border": "none",
          "boxShadow": "0px 2px 8px rgba(30, 30, 30, 0.15)"
        },
        "secondary": {
          "backgroundColor": "#FFFFFF",
          "color": "#5A5A5A",
          "border": "1px solid #E6E6E6",
          "borderRadius": "20px",
          "padding": "8px 16px",
          "fontSize": "12px",
          "fontWeight": 500
        },
        "icon": {
          "backgroundColor": "#1E1E1E",
          "color": "#FFFFFF",
          "borderRadius": "50%",
          "width": "32px",
          "height": "32px",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "center"
        },
        "quantity": {
          "backgroundColor": "transparent",
          "color": "#5A5A5A",
          "border": "1px solid #E6E6E6",
          "borderRadius": "4px",
          "width": "32px",
          "height": "32px",
          "fontSize": "14px"
        }
      },
      "card": {
        "product": {
          "backgroundColor": "#FFFFFF",
          "borderRadius": "12px",
          "padding": "0px",
          "boxShadow": "0px 2px 8px rgba(0, 0, 0, 0.04)",
          "border": "1px solid #F0F0F0",
          "overflow": "hidden"
        },
        "surface": {
          "backgroundColor": "#FFFFFF",
          "borderRadius": "8px",
          "padding": "16px",
          "border": "1px solid #E6E6E6"
        }
      },
      "colorSwatch": {
        "width": "24px",
        "height": "24px",
        "borderRadius": "50%",
        "border": "2px solid #FFFFFF",
        "boxShadow": "0px 1px 3px rgba(0, 0, 0, 0.1)"
      },
      "rating": {
        "starSize": "14px",
        "gap": "2px",
        "fontSize": "12px",
        "color": "#5A5A5A"
      },
      "navigation": {
        "height": "64px",
        "backgroundColor": "#FFFFFF",
        "borderBottom": "1px solid #E6E6E6",
        "padding": "0px 24px"
      },
      "section": {
        "padding": "32px 24px",
        "maxWidth": "1200px",
        "margin": "0 auto"
      }
    },
    "layout": {
      "containerWidth": "1200px",
      "gridGap": "24px",
      "mobileBreakpoint": "768px",
      "tabletBreakpoint": "1024px",
      "desktopBreakpoint": "1200px"
    },
    "animations": {
      "duration": {
        "fast": "150ms",
        "normal": "300ms",
        "slow": "500ms"
      },
      "easing": {
        "default": "cubic-bezier(0.4, 0, 0.2, 1)",
        "in": "cubic-bezier(0.4, 0, 1, 1)",
        "out": "cubic-bezier(0, 0, 0.2, 1)"
      }
    }
  }